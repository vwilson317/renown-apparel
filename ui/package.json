{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "build:docker": "npm run build && npm run docker:compose",
    "docker:compose": "npm run dockerfile:move && docker-compose -f ./dist/docker-compose.yml up --build",
    "dockerfile:move": "cp docker/* dist",
    "docker:down": "docker-compose down",
    "docker:tag:ui": "docker tag renown-apparel-ui:latest 475795201917.dkr.ecr.us-east-2.amazonaws.com/renown-apparel-ui:latest",
    "docker:tag:api": "docker tag renown-apparel-api:latest 475795201917.dkr.ecr.us-east-2.amazonaws.com/renown-apparel-api:latest",
    "docker:push:ui": "docker push 475795201917.dkr.ecr.us-east-2.amazonaws.com/renown-apparel-ui:latest",
    "docker:push:api": "docker push 475795201917.dkr.ecr.us-east-2.amazonaws.com/renown-apparel-api:latest",
    "docker:deploy": "npm run docker:tag:ui && npm run docker:tag:api && npm run docker:push:ui && npm run docker:push:api",
    "docker:login": "(aws ecr get-login --no-include-email)",
    "test": "jest",
    "run:db": "docker run -p 8086:6379 redis:5.0.6-alpine"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@types/jest": "^24.9.1",
    "axios": "^0.19.2",
    "bootstrap-vue": "^2.15.0",
    "core-js": "^2.6.11",
    "jest": "^24.9.0",
    "jquery": "^3.5.1",
    "ts-jest": "^24.3.0",
    "v-viewer": "^1.5.1",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "typescript": "^3.9.5",
    "vue-template-compiler": "^2.6.11"
  }
}
